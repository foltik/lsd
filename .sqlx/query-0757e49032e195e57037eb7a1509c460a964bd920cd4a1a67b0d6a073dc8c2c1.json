{
  "db_name": "SQLite",
  "query": "\n            INSERT INTO emails (kind, address, post_id, list_id)\n                SELECT ?, lm.email, ?, lm.list_id\n                FROM list_members lm\n                WHERE lm.list_id = ?\n                  AND NOT EXISTS (\n                      SELECT 1\n                      FROM emails e\n                      WHERE e.address = lm.email\n                        AND e.post_id = ?\n                        AND e.list_id = lm.list_id\n                        AND e.sent_at IS NULL\n                  )\n            RETURNING *;\n            ",
