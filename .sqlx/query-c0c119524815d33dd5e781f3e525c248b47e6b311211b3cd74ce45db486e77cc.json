  "query": "SELECT\n                r.id AS rsvp_id,\n                r.user_id,\n                s.name AS spot_name,\n                u.first_name,\n                u.last_name,\n                u.email,\n                u.phone,\n                r.contribution\n               FROM rsvps r\n               JOIN spots s ON s.id = r.spot_id\n               JOIN rsvp_sessions rs ON rs.id = r.session_id\n               LEFT JOIN users u ON u.id = r.user_id\n               WHERE rs.id = ?\n            ",
