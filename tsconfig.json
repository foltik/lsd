{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "Node",
        "outDir": "frontend/static/dist",
        "rootDir": "frontend/src",
        "strict": true,
        "esModuleInterop": true,
        "lib": [
            "ES2020",
            "DOM"
        ]
    },
    "include": [
        "frontend/src/**/*"
    ],
    "exclude": [
        "node_modules",
        "frontend/static/dist"
    ]
}